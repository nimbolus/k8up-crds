{
  "enabled": true,
  "extends": [
    "config:base"
  ],
  "regexManagers": [
    {
      "fileMatch": [
        "^Makefile$"
      ],
      "matchStrings": [
        "registryUrl=(?<registryUrl>.*?) chart=(?<depName>.*?)\n.*=(?<currentValue>.*?)\\/"
      ],
      "datasourceTemplate": "helm"
    }
  ]
}